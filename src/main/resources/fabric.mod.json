{
	"schemaVersion": 1,
	"id": "woolproofed",
	"version": "${version}",
	"name": "Woolproofed",
	"description": "A mod that blocks sounds when wool is placed between the sound source and the player.",
	"authors": [
		"SSnowly",
		"clamytoe"
	],
	"contact": {
		"homepage": "https://github.com/SSnowly/woolproofed",
		"sources": "https://github.com/SSnowly/woolproofed"
	},
	"license": "CC0-1.0",
	"icon": "assets/woolproofed/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"hu.snowylol.woolproofed.Woolproofed"
		],
		"client": [
			"hu.snowylol.woolproofed.WoolproofedClient"
		]
	},
	"mixins": [
		"woolproofed.mixins.json",
		{
			"config": "woolproofed.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.1",
		"java": ">=17",
		"fabric-api": ">=0.102.1+1.21.1"
	},
	"suggests": {
		"another-mod": "*"
	}
}
